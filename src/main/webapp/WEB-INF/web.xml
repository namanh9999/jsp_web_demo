<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>
  <servlet>
  	<servlet-name>login</servlet-name>
  	<display-name>login</display-name>
  	<description></description>
  	<servlet-class>controller.login</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>registerControl</servlet-name>
  	<display-name>registerControl</display-name>
  	<description></description>
  	<servlet-class>controller.registerControl</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>loginControl</servlet-name>
  	<display-name>loginControl</display-name>
  	<description></description>
  	<servlet-class>controller.loginControl</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>logoutControl</servlet-name>
  	<display-name>logoutControl</display-name>
  	<description></description>
  	<servlet-class>controller.logoutControl</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>changePass</servlet-name>
  	<display-name>changePass</display-name>
  	<description></description>
  	<servlet-class>controller.changePass</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>login</servlet-name>
  	<url-pattern>/login</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>registerControl</servlet-name>
  	<url-pattern>/registerControl</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>loginControl</servlet-name>
  	<url-pattern>/loginControl</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>logoutControl</servlet-name>
  	<url-pattern>/logoutControl</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>changePass</servlet-name>
  	<url-pattern>/changePass</url-pattern>
  </servlet-mapping>
  <error-page>
  	<error-code>404</error-code>
  	<location>/404Page.jsp</location>
  </error-page>
 <error-page>
  	<error-code>403</error-code>
  	<location>/403.jsp</location>
  </error-page>
 <error-page>
  	<error-code>500</error-code>
  	<location>/500.jsp</location>
  </error-page>

</web-app>